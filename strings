       //
      // let x = "1@345@5678@12234"
       function validate(num){
        let str = '';
        for(i=0;i<num.length;i++){
            if(num[i]!=="@"){
                str += num[i];
            }
        }
        console.log(str);
       }
    let x = validate("1@234@5678@23456");
